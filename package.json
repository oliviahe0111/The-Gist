{
  "name": "the-gist-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "AI-powered news app with Instagram Stories interface",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "test": "turbo run test",
    "web:dev": "cd apps/web && npm run dev",
    "mobile:dev": "cd apps/mobile && npx expo start",
    "web:build": "cd apps/web && npm run build",
    "mobile:build": "cd apps/mobile && npx expo build"
  },
  "dependencies": {},
  "devDependencies": {
    "turbo": "^2.3.3",
    "typescript": "^5.7.3",
    "@types/node": "^22.10.6"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "npm@10.8.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/oliviahe0111/The-Gist.git"
  },
  "author": "oliviahe0111",
  "license": "MIT"
}
